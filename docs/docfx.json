{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/office/dev/scripts/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "ms.suite": "office",
      "ms.author": "o365devx",
      "author": "o365devx",
      "ms.topic": "conceptual",
      "ms.technology": "scripts",
      "ms.prod": "excel",
      "titleSuffix": "Office Scripts"
    },
    "fileMetadata": {
      "ms.topic": {
        "tutorials/*.md": "tutorial"
      }
    },
    "template": [],
    "dest": "office-scripts-docs",
    "markdownEngineName": "markdig"
  }
}